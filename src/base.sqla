type Bool=True|False

not = \x -> match (x) {True -> False;False -> True}

or = \x -> \y -> match (x) {True -> True;False -> match (y) {True -> True;False -> False}}

and = \x -> \y -> match (x) {False -> False;True -> match (y) {True -> True;False -> False}}

empty = {}


identity = \x -> x

fst = \x -> \y -> x

snd = \x -> \y -> y
